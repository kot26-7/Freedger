<div id='ddln-form'>
  <div class='row'>
    <% alerts.each do |deadline| %>
      <div class='col-md-6 col-lg-4 <%= alert_define(deadline) %>'>
        <%= render 'deadline_fields', deadline: deadline %>
      </div>
    <% end %>
  </div>
  <div id='pagenation-zone'>
    <%= paginate alerts, remote: true %>
  </div>
</div>